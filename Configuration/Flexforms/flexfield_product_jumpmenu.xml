            <option_enable>
                <TCEforms>
                    <label>Enable Jumpmenu</label>
                    <config>
                        <type>check</type>
                        <defaul>0</defaul>
                    </config>
                </TCEforms>
            </option_enable>

            <option_producttitle>
                <TCEforms>
                    <label>Override Product Title (should be short). If not set, the current page title is used.</label>
